# Aliases

# Show current UNIX timestamp.
alias ut="date +%s"

# Always list files in long format.
alias ls="ls -l"

# Find files.
alias ff="find . -type f -name"

# Find directories.
alias fd="find . -type d -name"

# Fix my common mistakes.
alias claer="clear"

# Jekyll aliases

# Serve
alias jks="jekyll serve"

# Build
alias jkb="jekyll build"
